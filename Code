import java.util.Arrays;


public class Solution {

	public static boolean isAllPossibilities(int[] arg) {
    
    boolean flag = false;
    
    Arrays.sort(arg);
   
    for(int i = 0; i<arg.length; i++){
   if(arg[i] == i){
     flag = true;
   } else{
     flag = false;
     break;
   }
    }
		// Your code here...
    return flag;
	}

}
